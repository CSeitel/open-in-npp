{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Some",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      , "${workspaceFolder}/src/test/suite/cliArguments/workspaces/Some.code-workspace"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: watch"
    }
  , {
      "name": "CLI",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      , "${workspaceFolder}/src/test/suite/cliArguments/workspaces/CLI.code-workspace"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "npm: watch"
    }
  , {
      "name": "Notepad",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      , "${workspaceFolder}/src/test/suite/cliArguments/workspaces/Notepad.code-workspace"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "npm: watch"
    }
  , {
      "name": "GVim",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      , "${workspaceFolder}/src/test/suite/cliArguments/workspaces/GVim.code-workspace"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "npm: watch"
    }

  , {
      "name": "Extension Tests",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      , "--extensionTestsPath=${workspaceFolder}/out/test/suite/index"
      , "${workspaceFolder}/src/test/suite/cliArguments/Some.code-workspace"
      , "${workspaceFolder}/src/test/suite/cliArguments/Some.json"
      , "--disable-extensions"
      ],
      "outFiles": [
        "${workspaceFolder}/out/test/**/*.js"
      ],
      "preLaunchTask": "npm: watch"
    }
  , {
      "name": "Mocha Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "bdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceFolder}/out/test/mocha/**/*.spec.js"
      ]
    , "cwd": "${workspaceFolder}"
    , "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}