{
  "treeTemplate": {
    "excludeBaseFolders": [".git",".vscode-test","node_modules"]
  }
, "trees": [
    { "label": "Source-Code"
    , "expandBaseFolder": true
    , "autoReveal": 9
    , "baseFolder"        : "src"
    , "excludeBaseFolders": "test"
    , "associatedFolder"  : "out"
    , "fileExtensionMappings": "ts:js"
    }
  , { "prependHeader": true
    , "label": "Test-Code"
    , "baseFolder"      : "src/test"
    , "associatedFolder": "out/test/"
    }

  , { "prependHeader": true
    , "label": "Config-Files"
    , "baseFolderPattern": ".vscode"
    , "filePattern": "*.json"
    , "excludeFiles": "package-lock.json"
    , "associatedFolder": "../any-tree"
    }
  , { "prependHeader": false
    , "label": "Markdown"
    , "baseFolder": "./"
    , "filePattern": "*.md"
    }

  , { "prependHeader": true
    , "label": "Test-Workspace"
    , "baseFolder": "src/test/suite/cliArguments"
    , "filePattern_": "*.code-workspace"
    }

  , { "prependHeader": true
    , "autoReveal": 9
    , "label": "Compiled-Code"
    , "baseFolder"        : "out"
    , "excludeBaseFolders": "test"
    , "associatedFolder"  : "src"
    , "fileExtensionMappings": "ts:js"
    , "excludeFiles": "*.map"
    }

  , { "prependHeader": true
    , "label": "any-tree"
    ,    "baseFolder": "../any-tree/src"
    , "associatedFolder": "../open-in-npp/src"
    }
  , { "prependHeader": false
    , "label": "sap-logon"
    , "baseFolder": "../sap-logon"
    , "repository": "https://github.com/CSeitel/sap-logon"
    }
  ]
}